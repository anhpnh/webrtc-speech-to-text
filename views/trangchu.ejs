<!DOCTYPE html>
<html>

<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Demo WebRTC Speech To Text</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='/css/main.css'>
    <script src="/js/jquery-3.5.1.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/peerjs.min.js"></script>
    <script src="/js/p5.js"></script>
    <script src="/js/p5.sound.js"></script>
    <script src="/js/p5.speech.js"></script>

</head>

<body>
    <div id="divLocal">
        <video id="localStream" controls muted></video>
        <div id="txtLocalSpeech"></div>
    </div>
    <div id="divSignUp">
        <input type="text" id="txtUserName" placeholder="Username" />
        <button id="btnSignUp">SignUp</button>
    </div>
    <div id="divOnline">
        <p>Online users - Click to Call</p>
        <ul id="ulUser">
        </ul>
    </div>

    <div id="divRemote">
        <video id="remoteStream" controls></video>
        <div id="txtRemoteSpeech"></div>
    </div>


</body>
<script src='/js/bundle.js'></script>

</html>